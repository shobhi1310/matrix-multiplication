#include<stdio.h>

int main(void)
{

int a[12];

for (int i=1;i<=12;i++)
{scanf("%d",&a[i-1]);} 

printf("A:-\n");
for(int i=1;i<=12;i++)
{
printf("%d  ",a[i-1]);
if((i%4)==0)
{printf("\n");}
}

printf("\n\nB:-\n");

for(int i =1;i<=4;i++)
{
for(int j=i;j<=i+8;j+=4)
{printf("%d ",a[j-1]);}
printf("\n");
}

}

